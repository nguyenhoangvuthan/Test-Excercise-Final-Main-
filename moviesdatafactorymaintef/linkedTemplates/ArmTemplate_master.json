{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "moviesdatafactorymaintef"
		},
		"AzureSqlDatabase_Service_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase_Service'"
		},
		"BG_Cosmos_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BG_Cosmos'"
		},
		"Blob_Raw_Layer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Blob_Raw_Layer'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "moviesdatafactorymaintef_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase_Service_connectionString": {
						"value": "[parameters('AzureSqlDatabase_Service_connectionString')]"
					},
					"BG_Cosmos_connectionString": {
						"value": "[parameters('BG_Cosmos_connectionString')]"
					},
					"Blob_Raw_Layer_connectionString": {
						"value": "[parameters('Blob_Raw_Layer_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}